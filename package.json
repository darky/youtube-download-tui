{
  "name": "ytdl-tui",
  "version": "0.9.3",
  "description": "TUI for downloading Youtube videos",
  "repository": {
    "type": "git",
    "url": "git://github.com/darky/ytdl-tui.git"
  },
  "files": [
    "dist/index.js"
  ],
  "bin": {
    "ytdl-tui": "./dist/index.js"
  },
  "scripts": {
    "build": "rm -rf dist && npx tsc && sed -i '1 i #!/usr/bin/env node' dist/index.js && chmod +x dist/index.js",
    "dev": "node --inspect -r ts-node/register/transpile-only -r tsconfig-paths/register src/index.tsx",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "youtube",
    "download",
    "tui"
  ],
  "author": "Vladislav Botvin",
  "license": "MIT",
  "dependencies": {
    "ink": "^3.2.0",
    "ink-big-text": "^1.2.0",
    "ink-form": "^1.0.2",
    "react": "^17.0.2",
    "repl-ns": "^0.0.15",
    "rocket-pipes-slim": "^0.11.0",
    "ytdl-core-muxer": "^1.0.1"
  },
  "devDependencies": {
    "@types/ink-big-text": "1.2.1",
    "@types/react": "17.0.39",
    "ink-testing-library": "2.1.0",
    "node-remote-repl": "1.1.0",
    "prettier": "2.8.0",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.1",
    "typescript": "4.9.3",
    "uvu": "0.5.6"
  }
}
