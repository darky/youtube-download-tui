{
  "name": "ytdl-tui",
  "version": "0.9.3",
  "description": "TUI for downloading Youtube videos",
  "repository": {
    "type": "git",
    "url": "git://github.com/darky/ytdl-tui.git"
  },
  "files": [
    "src"
  ],
  "bin": {
    "ytdl-tui": "./run.sh"
  },
  "scripts": {
    "dev": "nbb --classpath \"src\" nrepl-server",
    "start": "nbb --classpath \"src\" src/main.cljs",
    "test": "nbb --classpath \"src:test\" test/maintest.cljs"
  },
  "keywords": [
    "youtube",
    "download",
    "tui"
  ],
  "author": "Vladislav Botvin",
  "license": "MIT",
  "dependencies": {
    "ink": "^3.2.0",
    "ink-big-text": "^1.2.0",
    "ink-form": "^1.0.2",
    "repl-ns": "^0.0.15",
    "rocket-pipes-slim": "^0.11.0",
    "ytdl-core-muxer": "^1.0.1"
  },
  "devDependencies": {
    "ink-testing-library": "2.1.0",
    "node-remote-repl": "1.0.0",
    "prettier": "2.8.0",
    "tsx": "3.12.1",
    "typescript": "4.9.3",
    "uvu": "0.5.6"
  }
}
